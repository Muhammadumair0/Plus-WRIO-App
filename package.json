    "domready": "^1.0.8",
    "ejs": "1.0.0",
    "envify": "^3.4.0",
    "es6-promise": "^2.3.0",
    "express": "4.9.4",
    "express-session": "*",
    "gulp": "^3.9.0",
    "gulp-browserify": "^0.5.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.0",
    "gulp-wrapper": "^1.0.0",
    "lodash.sortby": "^3.1.4",
    "lodash.some": "*",
    "nconf": "*",
