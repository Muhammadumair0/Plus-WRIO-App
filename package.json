{
  "name": "plus",
  "version": "0.0.19",
  "description": "(coming soon)",
  "repository": {
    "type": "git",
    "url": "https://github.com/webRunes/Plus-WRIO-App"
  },
  "author": "",
  "license": "ISC",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "scripts": {
    "postinstall": "npm run buildHub",
    "start": "npm run lint && npm run buildTest",
    "lint": "eslint js",
    "build": "npm run buildTest & npm run buildHub",
    "buildHub": "echo '<script>' > widget/storageHub.htm && browserify -g uglifyify js/hub.js >> widget/storageHub.htm && echo '</script>' >> widget/storageHub.htm",
    "buildTest": "browserify -t [envify --NODE_ENV development] -t reactify test/index.js -o test/bundle.js",
    "watch": "watchify -p browserify-notify -t [envify --NODE_ENV development] -t reactify test/index.js -o test/bundle.js -d",
    "test": "",
    "bumpVersion": "npm run version && npm run bundle",
    "version": "sh version.sh",
    "bundle": "sh bundle.sh"
  },
  "dependencies": {
    "aws-sdk-apis": "*",
    "body-parser": "1.7.0",
    "classnames": "^2.1.2",
    "cookie-parser": "1.3.2",
    "cross-storage": "^0.7.0",
    "domready": "^1.0.8",
    "ejs": "1.0.0",
    "es6-promise": "^2.3.0",
    "express": "4.9.4",
    "express-session": "*",
    "lodash.sortby": "^3.1.4",
    "nconf": "*",
    "passport": "0.2.1",
    "passport-google-oauth": "*",
    "react": "^0.13.3",
    "reflux": "^0.2.7",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "browserify-notify": "^1.1.2",
    "envify": "^3.4.0",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^3.0.0",
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.2"
  }
}
